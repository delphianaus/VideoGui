<Window x:Name="wdwProgress" x:Class="VideoGui.ConverterProgressInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoGui"
        mc:Ignorable="d" Loaded="Window_Loaded" Closing="Window_Closing" 
        Title="Converter Progress Info" Height="450" Width="800" WindowStartupLocation="CenterScreen" 
        ResizeMode="CanResizeWithGrip" SizeChanged="wdwProgress_SizeChanged" WindowStyle="ToolWindow">
    <Grid>
        <Border Name="brd1" BorderThickness="2" BorderBrush="Black" Margin="0,0,0,49" Padding="2,2,2,2">
            <ListBox Name="ItemProgress" VerticalAlignment="Top" HorizontalAlignment="Left" Width="791" 
                     Height="376">


            </ListBox>
        </Border>
        <Button x:Name="BtnClose" Content="Close" Width="90" Height="30" Margin="0,0,9,10" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="BtnClose_Click"/>
        <Label x:Name="lblstatus" Content="" Height="22" Margin="10,396,562,16"/>
    </Grid>
</Window>
