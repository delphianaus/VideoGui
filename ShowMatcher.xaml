<Window x:Name="frmShowMatcher" x:Class="VideoGui.ShowMatcher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoGui"
        mc:Ignorable="d"
        Title="Show Matcher" Height="110" Width="525" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen"  Closing="frmShowMatcher_Closing">
    <Grid Margin="0,0,0,9">
        <Button Content="Load JSON" HorizontalAlignment="Left" Height="27" Margin="10,15,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click"/>
        <Label x:Name="LblFiles" Content="Label" HorizontalAlignment="Left" Margin="105,15,0,0" VerticalAlignment="Top" Height="27"/>
        <Button x:Name="BtnLoadFileList" Content="Load File List" Margin="0,15,114,0" Click="Button_Click_1" HorizontalAlignment="Right" VerticalAlignment="Top" Width="90" Height="27" IsEnabled="False"/>
        <Button x:Name="BtnMatchNames" Content="Match Names" Margin="0,15,10,0" Click="BtnMatchNames_Click" HorizontalAlignment="Right" VerticalAlignment="Top" Width="99" Height="26" IsEnabled="False"/>

    </Grid>
</Window>
