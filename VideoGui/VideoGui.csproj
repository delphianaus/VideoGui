    <Resource Include="Icons\computer1.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>

    <Resource Include="Icons\computer2.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>

    <Resource Include="Icons\computer4.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
