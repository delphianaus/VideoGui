<Window x:Name="frmEnterString" x:Class="VideoGui.CustomStringEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoGui"
        mc:Ignorable="d" Height="95" Width="301" WindowStyle="ToolWindow" ResizeMode="NoResize" 
       WindowStartupLocation="CenterScreen" Topmost="True" 
        Title="CustomStringEntry" >
    <Grid>
        <Label Content="Data :" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="41"/>
        <TextBox x:Name="txtData" HorizontalAlignment="Left" Margin="76,4,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="211" KeyDown="txtData_KeyDown"/>
        <Button x:Name="BtnClose" Content="Close" HorizontalAlignment="Left" Margin="247,28,0,0" VerticalAlignment="Top" FontSize="10" Width="40" Height="22" Click="BtnClose_Click" />
    </Grid>
</Window>
